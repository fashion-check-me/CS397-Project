<div id="content_wrapper">
  <div id="female" class="gender_pane">
    <div id="best_female" class="female_tiles best_tile outfit_tile">
        <%= image_tag "femaleTop.jpg", :class => "top_image" %>
        <%= image_tag "upvote.png", :class => "upvote_image" %>
        <%= image_tag "downvote.png", :class => "downvote_image" %>
        <%= image_tag "favorite.png", :class => "favorite_image" %>
    </div>
    <div class="female_tiles">
      <% 4.times do |i| %>
      <div class="outfit_tile">
        <%= image_tag "female#{i}.jpg" %>
        <%= image_tag "upvote.png" %>
        <%= image_tag "downvote.png" %>
        <%= image_tag "favorite.png" %>
      </div>
      <% end %>
    </div>
  </div>

  <div id="male" class="genderPane">
    <div id="best_male" class="male_tiles best_tile outfit_tile">
        <%= image_tag "maleTop.jpg", :class => "top_image" %>
        <%= image_tag "upvote.png", :class => "upvote_image" %>
        <%= image_tag "downvote.png", :class => "downvote_image" %>
        <%= image_tag "favorite.png", :class => "favorite_image" %>
    </div>
    <div class="male_tiles">
      <% 4.times do |i| %>
      <div class="outfit_tile">
        <%= image_tag "male#{i}.jpg" %>
        <%= image_tag "upvote.png" %>
        <%= image_tag "downvote.png" %>
        <%= image_tag "favorite.png" %>
      </div>
      <% end %>
    </div>
  </div>
</div>
